<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.handlerdemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/restart"
        android:label="数据存储"
        android:roundIcon="@drawable/fengxing"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SaveFileActivity"></activity>
        <activity android:name=".qqLoginActivity"
            android:label="QQ2020"/>
        <activity
            android:name=".MenuActivity"
            android:label="主界面">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Handler下载"
            android:roundIcon="@drawable/souhumovie" />

        <!--    在SDCard中创建与删除文件权限-->

    </application>
    <uses-permission android:name="android:permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="WrongManifestParent" />
    <!--    往SDCard写入数据权限-->
    <uses-permission android:name="android:permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="WrongManifestParent" />
</manifest>